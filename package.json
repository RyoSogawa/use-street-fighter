{
  "name": "use-street-fighting-command",
  "version": "0.1.0",
  "description": "React custom hooks for detecting Street Fighter-style command inputs",
  "keywords": [
    "react",
    "react-hooks",
    "street-fighter",
    "fighting-game",
    "command-input"
  ],
  "author": "RyoSogawa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyoSogawa/use-street-fighting-command.git"
  },
  "bugs": {
    "url": "https://github.com/RyoSogawa/use-street-fighting-command/issues"
  },
  "homepage": "https://use-street-fighting-command.vercel.app/",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "pnpm --filter demo dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "prerelease": "pnpm test && pnpm build",
    "release:patch": "pnpm prerelease && npm version patch",
    "release:minor": "pnpm prerelease && npm version minor",
    "release:major": "pnpm prerelease && npm version major"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.14",
    "jsdom": "^27.3.0",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.15"
  },
  "volta": {
    "node": "22.18.0",
    "pnpm": "10.25.0"
  }
}
